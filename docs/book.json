{
    "gitbook": "3.x",
    "title": "ZhiZhouDevelopmentSpecification",
    "description": "芝舟开发规范",
    "language": "zh-hans",
    "structure": {
        "readme": "SUMMARY.md"
    },
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        }
    },
    "generate": "site",
    "plugins": [
        "-sharing",
        "-livereload",
        "-search",
        "-fontsettings"
    ],
    "pdf": {
        "toc": true,
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": true,
        "paperSize": "a4"
    }
}